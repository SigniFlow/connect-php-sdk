{
    "name": "signiflow/connect-php-sdk",
    "version": "1.20210712.0",
    "description": "## SigniFlow API used to automate esignature workflow creation and management.",
    "keywords": [
        "signiflow",
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://www.signiflow.com",
    "license": "mit",
    "authors": [
        {
            "name": "SigniFlow",
            "homepage": "https://github.com/SigniFlow/connect-php-sdk"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "SigniFlowConnect\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "SigniFlowConnect\\Test\\" : "test/" }
    }
}
